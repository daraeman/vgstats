{
  "name": "vg_stats",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "setup": "node server/setup.js",
    "start_process_markets": "node server/index.js start process_markets",
    "stop_process_markets": "node server/index.js stop process_markets",
    "status_process_markets": "node server/index.js status process_markets",
    "start_fetch_markets": "node server/index.js start fetch_markets",
    "stop_fetch_markets": "node server/index.js stop fetch_markets",
    "status_fetch_markets": "node server/index.js status fetch_markets",
    "start_database": "if [ -z $(pidof mongod) ]; then mongod --config mongo.conf; else echo 'Mongod is already running'; fi;",
    "stop_database": "if [ ! -z $(pidof mongod) ]; then kill $(pidof mongod); else echo 'Mongod was not running'; fi;",
    "start_client_dev": "export NODE_ENV=dev; node_modules/.bin/webpack-dev-server",
    "start_server_dev": "export NODE_ENV=dev; nodemon server/app/web_server.js",
    "start_server": "export NODE_ENV=production; node server/index.js start web_server",
    "stop_server": "export NODE_ENV=production; node server/index.js stop web_server",
    "build_client": "export NODE_ENV=production; webpack --config webpack.config.js",
    "manage_feeds": "node server/app/manage_feeds.js"
  },
  "author": "David Rae",
  "license": "MIT",
